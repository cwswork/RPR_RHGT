{
	"datasetPath": "../../0908datasets/",
	"output": "../../1212output/",  //1127output  0908output
	"seed": 72,
	// 维度
	"e_dim": 300,
	"n_layers": 1,
	"n_heads": 4,
	"embed_type": 1, //名称嵌入选择
	//"beta1": 1.0,  // 0.1、1、1.5 => 1
	"l_beta": 0.3, // 0.4, 0.5
	"gamma_rel": 10.0,  // 0.1、1、1.5 => 1

	// 训练
	"optim_type": "Adagrad",  //1000,500
	"metric": "L1",	 // L1  L2  cosine
  	"train_epochs": 4000,  //1000,500

	//验证
	"early_stop": true,
	"start_valid": 60,  //验证起始
	"eval_freq": 10,   // 验证==采样频率
	"eval_save_freq": 10,  //保存频率
	"patience": 20,
	"patience_minloss": 50, //40

	//采样
	"neg_k": 50,

	//超参
	"learning_rate": 0.001,
	"weight_decay": 5e-4,
	"LeakyReLU_alpha": 0.2,
	"dropout": 0.1,   // 0.1, 0.2, 0.3
	"top_k": [1, 3, 5, 10, 50, 100] // [1, 3, 5, 10, 50, 100]
}

