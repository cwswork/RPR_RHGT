{
	"datasetPath": "../../datasets/",
	"output": "../../1212output/",
	"seed": 72,
	// dim
	"e_dim": 300,
	"n_layers": 1,
	"n_heads": 4,
	"embed_type": 1, 
	//"beta1": 1.0, 
	"l_beta": 0.3,
	"gamma_rel": 10.0,  

	// train
	"optim_type": "Adagrad", 
	"metric": "L1",	 // L1  L2  cosine
  	"train_epochs": 4000,  //1000,500

	// valid
	"early_stop": true,
	"start_valid": 60,  
	"eval_freq": 10,  
	"eval_save_freq": 10, 
	"patience": 20,
	"patience_minloss": 50, 

	//sample
	"neg_k": 50,

	//super
	"learning_rate": 0.001,
	"weight_decay": 5e-4,
	"LeakyReLU_alpha": 0.2,
	"dropout": 0.1,
	"top_k": [1, 3, 5, 10, 50, 100] 
}

